{
  "name": "wallcrawler-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "AI-Powered Browser Automation Framework - Monorepo",
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r --parallel run dev",
    "test": "pnpm -r run test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "pnpm -r run typecheck",
    "clean": "pnpm -r exec rm -rf dist node_modules",
    "format": "prettier --write \"packages/**/*.{ts,tsx,js,jsx,json,md}\" \"*.{ts,tsx,js,jsx,json,md}\" \"!packages/stagehand/**\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Volpestyle/wallcrawler.git"
  },
  "keywords": [
    "browser-automation",
    "playwright",
    "ai",
    "llm",
    "vercel",
    "web-scraping",
    "automation"
  ],
  "author": "James Volpe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Volpestyle/wallcrawler/issues"
  },
  "homepage": "https://github.com/Volpestyle/wallcrawler#readme",
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@eslint/js": "^9.16.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.3.4",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.17.0"
  }
}
