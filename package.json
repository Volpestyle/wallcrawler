{
    "name": "wallcrawler",
    "version": "0.1.0",
    "description": "Serverless browser automation platform using AWS and Stagehand",
    "private": true,
    "scripts": {
        "build": "pnpm -r build",
        "test": "pnpm -r test",
        "dev": "pnpm -r dev",
        "lint": "pnpm -r lint",
        "clean": "pnpm -r clean",
        "install:submodules": "git submodule update --init --recursive",
        "deploy": "pnpm --filter @wallcrawler/aws-cdk deploy",
        "deploy:ci": "pnpm --filter @wallcrawler/aws-cdk deploy:ci",
        "cdk": "pnpm --filter @wallcrawler/aws-cdk cdk",
        "generate-env": "pnpm --filter @wallcrawler/aws-cdk generate-env",
        "cdk:dev": "pnpm --filter @wallcrawler/aws-cdk deploy:dev",
        "cdk:staging": "pnpm --filter @wallcrawler/aws-cdk deploy:staging",
        "cdk:prod": "pnpm --filter @wallcrawler/aws-cdk deploy:prod"
    },
    "keywords": [
        "browser-automation",
        "serverless",
        "aws",
        "stagehand",
        "monorepo"
    ],
    "author": "Wallcrawler Team",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.0.0",
        "eslint": "^8.0.0",
        "prettier": "^3.0.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    },
    "packageManager": "pnpm@10.13.1"
}
