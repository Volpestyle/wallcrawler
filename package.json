{
  "name": "wallcrawler",
  "version": "0.1.0",
  "description": "AI-Powered Browser Automation Framework with Vercel AI SDK & Playwright",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Volpestyle/wallcrawler.git"
  },
  "keywords": [
    "browser-automation",
    "playwright",
    "ai",
    "llm",
    "vercel",
    "web-scraping",
    "automation"
  ],
  "author": "James Volpe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Volpestyle/wallcrawler/issues"
  },
  "homepage": "https://github.com/Volpestyle/wallcrawler#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.53.0",
    "ai": "^4.3.16",
    "openai": "^5.2.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "playwright": "^1.52.0",
    "zod": "^3.25.57"
  },
  "optionalDependencies": {
    "@ai-sdk/amazon-bedrock": "^2.2.10",
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.22",
    "ollama-ai-provider": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
