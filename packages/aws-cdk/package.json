{
    "name": "@wallcrawler/aws-cdk",
    "version": "0.1.0",
    "private": true,
    "description": "AWS CDK infrastructure for Wallcrawler remote browser automation",
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "npm run clean && tsc",
        "build:watch": "npm run clean && tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "deploy": "npm run build && cdk deploy --require-approval never",
        "destroy": "cdk destroy",
        "synth": "npm run build && cdk synth",
        "diff": "npm run build && cdk diff",
        "bootstrap": "cdk bootstrap"
    },
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/node": "20.6.2",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "typescript": "~5.2.2",
        "aws-cdk": "2.120.0",
        "ts-node": "^10.9.1"
    },
    "dependencies": {
        "aws-cdk-lib": "2.120.0",
        "constructs": "^10.0.0",
        "source-map-support": "^0.5.21"
    },
    "keywords": [
        "aws",
        "cdk",
        "infrastructure",
        "browser",
        "automation",
        "wallcrawler"
    ]
}