{
  "name": "@wallcrawler/infra-aws",
  "version": "0.1.0",
  "description": "AWS infrastructure provider for WallCrawler browser automation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@wallcrawler/infra-common": "workspace:*",
    "@wallcrawler/stagehand": "workspace:*",
    "@aws-sdk/client-ecs": "^3.0.0",
    "@aws-sdk/client-elasticache": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.0.0",
    "@aws-sdk/client-application-auto-scaling": "^3.0.0",
    "@aws-sdk/client-cloudwatch": "^3.0.0",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.8.0"
  },
  "keywords": [
    "aws",
    "ecs",
    "browser",
    "automation",
    "wallcrawler",
    "infrastructure"
  ],
  "author": "WallCrawler Team",
  "license": "MIT"
}
