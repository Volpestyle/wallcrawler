    "@wallcrawler/utils": "workspace:*",
    "jose": "^5.2.0",
    "playwright-core": "^1.40.0",
    "redis": "^5.6.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@wallcrawler/eslint-config": "workspace:*",
